<%= simple_form_for(profile) do |f| %>
  <%# <%= f.input :category, collection: Item::SPORT_TYPES %>
    <%= f.input :name %>
    <%= f.input :bio %>
    <%= f.input :age %>
    <%= f.input :address %>
    <%= f.input :native_language %>
    <%= f.input :second_language %>
    <%= f.input :photo, as: :file %>
  <%= f.submit "Update", :class => "btn btn-success" %>
  <%= link_to 'Cancel', root_path, class: 'btn btn-danger ml-2'%>
<% end %>

